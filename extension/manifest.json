{
  "manifest_version": 3,
  "name": "LinkedIn Job Scraper",
  "version": "1.0",
  "permissions": ["scripting", "tabs"],
  "host_permissions": ["https://www.linkedin.com/*"],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "action": {
    "default_popup": "popup.html"
  }
}
